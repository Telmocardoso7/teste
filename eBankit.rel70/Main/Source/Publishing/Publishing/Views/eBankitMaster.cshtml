@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = null;
    var CacheVersion = "ver=49";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>@Umbraco.Field("sEOTitle")</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
    <meta name="description" content='@Umbraco.Field("sEODescription")'>
    <meta name="keywords" content='@Umbraco.Field("sEOKeywords")'>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <link rel="shortcut icon" type="image/x-icon" href="/images/favicon.ico" />
    <!-- styles -->
    <link href="~/css/bootstrap/bootstrap.min.css" rel="stylesheet">
    <link href="~/css/ebankit/ebankit.css?@CacheVersion" rel="stylesheet"  media="screen">

     <!-- Custom Fonts -->
   
    <link href="http://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic" rel="stylesheet" type="text/css">
    <link href="http://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
 

    

</head>

<body id="page-top" data-spy="scroll" data-target=".navbar-fixed-top">
        
     @Html.Partial("Navigation", @Model.Content)

    <!-- Intro Header -->
    <header class="intro">
        <div class="intro-body">
            <div class="container">
                <div class="row">
                    <div class="col-md-8 col-md-offset-2">
                    
                 
                    </div>
                </div>
            </div>
        </div>
    </header>
    
   @RenderSection("Content")

   @Html.Partial("Footer", @Model.Content)
   
    <!-- jQuery Version 1.11.0 -->
    <script src="/scripts/jquery/jquery-1.11.0.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="/scripts/bootstrap/bootstrap.min.js"></script>

    <!-- Plugin JavaScript -->
    <script src="/scripts/jquery/jquery.easing.min.js"></script>

    
    <!-- Custom Theme JavaScript -->
    <script src="/scripts/ebankit/ebankit.js"></script>

</body>

</html>
