@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = null;
    var a = (this.CurrentPage);

    var b = a.GetPropertyValue("ebankIT_redirecturl");

    if (!string.IsNullOrEmpty(b))
    {
        Response.Redirect(b);
    }
    else
    {
        var l = CurrentPage.Children.Where("DocumentTypeAlias == \"ebankIT_Segment\"").FirstOrDefault();
        if (l != null)
        {
            Response.Redirect(l.Url);
        }
    }
}
