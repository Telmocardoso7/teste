          "CardsTransactions": "portal/sandbox/cards-transactions"
        }
      },
      "IdentityServer": {
        "Base": "api/Client",
        "Secret": "api/Client/UpdateClientSecret",
        "Scope": "api/Client/Scope"
      }
    }
  },
  "GlobalSettings": {
    "ServicesEndpoints": {
      "Platform": {
        "Cultures": {
          "Base": "/Platform/Cultures"
        },
        "Resources": {
          "Base": "/Platform/Resources"
        }
      }
    }
  }
}