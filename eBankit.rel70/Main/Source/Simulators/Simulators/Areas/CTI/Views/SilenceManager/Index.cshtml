@model ResumeViewModel

@{
    ViewData["Title"] = "CTI";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
    <div style="margin-left:10px;margin-right:10px;">
        <h2>Unidirectional CTI</h2>
        <br />
        <br />
        @{ await Html.RenderPartialAsync("_OutputMessages"); }
        <div>
            @{ await Html.RenderPartialAsync("IvrView", Model); }
            @{ await Html.RenderPartialAsync("UsersView", Model); }
        </div>
    </div>
    <environment include="Development">
        <script src="~/signalr/signalr.js"></script>
        <script src="~/js/areas/cti/SimulatorPlugins/signalR/simulator-hub.js"></script>
        <script src="~/js/areas/cti/simulator.js" type="text/javascript"></script>
        <script src="~/js/areas/cti/SimulatorPlugins/SimulatorServices.js"></script>
        <link href="~/css/ivr.css" rel="stylesheet" />
    </environment>
    <environment exclude="Development">
        <script src="~/signalr/signalr.min.js" asp-append-version="true"></script>
        <script src="~/js/areas/cti/SimulatorPlugins/signalR/simulator-hub.min.js" asp-append-version="true"></script>
        <script src="~/js/areas/cti/simulator.min.js" type="text/javascript" asp-append-version="true"></script>
        <script src="~/js/areas/cti/SimulatorPlugins/SimulatorServices.min.js" asp-append-version="true"></script>
        <link rel="stylesheet" href="~/css/ivr.min.css" asp-append-version="true" />
    </environment>
